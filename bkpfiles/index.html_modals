    <div class="portfolio-modal modal fade" id="fajr" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-content" style ='background-color:#BA4C63'>
	    <div class="close-modal" data-dismiss="modal">  
		<div class="lr">
